{
  "name": "mr.gil",
  "version": "0.0.4",
  "description": "A Guilded wrapper written in Typescript",
  "main": "lib/mrgil.js",
  "typings": "typings/mrgil.d.ts",
  "scripts": {
    "test": "node test",
    "dev": "npm publish --tag dev",
    "prepare": "husky install",
    "format": "npx pretty-quick"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mr-gil/mr.gil.git"
  },
  "keywords": [
    "guilded",
    "wrapper"
  ],
  "license": "CC BY-NC-ND 4.0",
  "author": "Rahuletto",
  "bugs": {
    "url": "https://github.com/mr-gil/mr.gil/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "homepage": "https://github.com/mr-gil/mr.gil#readme",
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@types/ws": "^8.5.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "guilded-api-typings": "^0.13.0",
    "typed-emitter": "^2.1.0",
    "ws": "^8.9.0"
  }
}
